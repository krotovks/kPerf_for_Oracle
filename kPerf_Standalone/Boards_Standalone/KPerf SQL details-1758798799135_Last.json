        {
          "id": "joinByField",
          "options": {
            "byField": "join_key",
            "mode": "inner"
          }
        },
        {
          "id": "groupBy",
          "options": {
            "fields": {
              "COMMAND_NAME": {
                "aggregations": [],
                "operation": "groupby"
              },
              "COMMAND_NAME 1": {
                "aggregations": [],
                "operation": "groupby"
              },
              "EVENT": {
                "aggregations": [],
                "operation": "groupby"
              },
              "EVENT 1": {
                "aggregations": [],
                "operation": "groupby"
              },
              "SID": {
                "aggregations": [],
                "operation": "groupby"
              },
              "SID 1": {
                "aggregations": [],
                "operation": "aggregate"
              },
              "SQLID": {
                "aggregations": [],
                "operation": "groupby"
              },
              "USERNAME": {
                "aggregations": [
                  "last"
                ],
                "operation": "groupby"
              },
              "USERNAME 2": {
                "aggregations": [],
                "operation": "aggregate"
              },
              "Value #A": {
                "aggregations": [
                  "last"
                ],
                "operation": "aggregate"
              },
              "Value #B": {
                "aggregations": [
                  "last"
                ],
                "operation": "aggregate"
              }
            }
          }
        },
        {
          "id": "organize",
          "options": {
            "excludeByName": {
              "COMMAND_NAME 2": true,
              "EVENT 2": true,
              "SID 2": true,
              "SQLID 2": true,
              "USERNAME (last) 1": true,
              "USERNAME 2": true
            },
            "includeByName": {},
            "indexByName": {
              "EVENT 1": 1,
              "EVENT 2": 2,
              "SQLID 1": 0,
              "SQLID 2": 3,
              "Value #A (last)": 4,
              "Value #B (last)": 5
            },
            "renameByName": {
              "COMMAND_NAME 1": "OPT",
              "EVENT 1": "EVENT",
              "SID 1": "SID",
              "SQLID 1": "SQLID",
              "USERNAME (last) 1": "",
              "USERNAME (last) 2": "User Name",
              "USERNAME 1": "User Name",
              "Value #A (last)": "Time Spent",
              "Value #B (last)": "Time Spent %"
            }
          }
