          "hide": false,
          "instant": false,
          "interval": "$minstep",
          "legendFormat": "__auto",
          "range": true,
          "refId": "A"
        },
        {
          "datasource": {
            "type": "prometheus",
            "uid": "${dsname}"
          },
          "editorMode": "code",
          "exemplar": false,
          "expr": "label_join(100 *\r\nsum(sum_over_time(SESSION_DELTA_EVENTS_TIME{job=\"$vsqlwait\",SQLID=~\"$SQLID\"} [$__range])) by (SQLID,EVENT,USERNAME,SID)\r\n/\r\nscalar(sum(sum_over_time(SESSION_DELTA_EVENTS_TIME{job=\"$vsqlwait\",SQLID=~\"$SQLID\"} [$__range]))),\"join_key\",\"_\", \"SQLID\",\"EVENT\",\"USERNAME\",\"SID\")",
